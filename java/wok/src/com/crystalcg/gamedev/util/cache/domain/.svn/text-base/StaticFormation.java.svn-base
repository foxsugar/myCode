package com.crystalcg.gamedev.util.cache.domain;

/**
 * 阵型表
 * 
 * @author xuzhongxing
 * 
 */
public class StaticFormation {
	private String formationNo;// 阵型编号
	private String formationIcon;// 阵型预览图标
	private String formationSmallIcon;// 阵型预览小图标
	private String name;// 阵型名
	private String coordinate1;// 部队1在阵型中的坐标
	private String coordinate2;// 部队2在阵型中的坐标
	private String coordinate3;// 部队3在阵型中的坐标
	private String coordinate4;// 部队4在阵型中的坐标
	private String coordinate5;// 部队5在阵型中的坐标
	private int applyToSoldier1;// 享受阵型影响的兵种1(1=刀兵 2=枪兵 3=骑兵 4=弓兵 5=器械)
	private int effect1;// 效果值1
	private int applyToSoldier2;// 享受阵型影响的兵种2
	private int effect2;// 效果值2
	private int applyToSoldier3;// 享受阵型影响的兵种3
	private int effect3;// 效果值3
	private String description;// 阵型描述
	private String effectDescription;// 阵型效果描述

	public String getFormationNo() {
		return formationNo;
	}

	public String getFormationIcon() {
		return formationIcon;
	}

	public String getName() {
		return name;
	}

	public String getCoordinate1() {
		return coordinate1;
	}

	public String getCoordinate2() {
		return coordinate2;
	}

	public String getCoordinate3() {
		return coordinate3;
	}

	public String getCoordinate4() {
		return coordinate4;
	}

	public String getCoordinate5() {
		return coordinate5;
	}

	public int getApplyToSoldier1() {
		return applyToSoldier1;
	}

	public int getEffect1() {
		return effect1;
	}

	public int getApplyToSoldier2() {
		return applyToSoldier2;
	}

	public int getEffect2() {
		return effect2;
	}

	public int getApplyToSoldier3() {
		return applyToSoldier3;
	}

	public int getEffect3() {
		return effect3;
	}

	public String getDescription() {
		return description;
	}

	public void setFormationNo(String formationNo) {
		this.formationNo = formationNo;
	}

	public void setFormationIcon(String formationIcon) {
		this.formationIcon = formationIcon;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setCoordinate1(String coordinate1) {
		this.coordinate1 = coordinate1;
	}

	public void setCoordinate2(String coordinate2) {
		this.coordinate2 = coordinate2;
	}

	public void setCoordinate3(String coordinate3) {
		this.coordinate3 = coordinate3;
	}

	public void setCoordinate4(String coordinate4) {
		this.coordinate4 = coordinate4;
	}

	public void setCoordinate5(String coordinate5) {
		this.coordinate5 = coordinate5;
	}

	public void setApplyToSoldier1(int applyToSoldier1) {
		this.applyToSoldier1 = applyToSoldier1;
	}

	public void setEffect1(int effect1) {
		this.effect1 = effect1;
	}

	public void setApplyToSoldier2(int applyToSoldier2) {
		this.applyToSoldier2 = applyToSoldier2;
	}

	public void setEffect2(int effect2) {
		this.effect2 = effect2;
	}

	public void setApplyToSoldier3(int applyToSoldier3) {
		this.applyToSoldier3 = applyToSoldier3;
	}

	public void setEffect3(int effect3) {
		this.effect3 = effect3;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getEffectDescription() {
		return effectDescription;
	}

	public void setEffectDescription(String effectDescription) {
		this.effectDescription = effectDescription;
	}

	public String getFormationSmallIcon() {
		return formationSmallIcon;
	}

	public void setFormationSmallIcon(String formationSmallIcon) {
		this.formationSmallIcon = formationSmallIcon;
	}

}
