package com.crystalcg.gamedev.util.cache.domain;

/**
 * 战斗物品奖励静态信息
 * 
 * @author jinganyang
 * 
 */
public class StaticArticleReward {

	private String itemRewardNo;// 物品奖励编号，可以重复
	private String itemNo; // 物品编号
	private int itemType; // 物品类型
	private int amount; // 奖励数量
	private double dropRate;// 掉率

	public String getItemRewardNo() {
		return itemRewardNo;
	}

	public String getItemNo() {
		return itemNo;
	}

	public int getItemType() {
		return itemType;
	}

	public int getAmount() {
		return amount;
	}

	public double getDropRate() {
		return dropRate;
	}

	public void setItemRewardNo(String itemRewardNo) {
		this.itemRewardNo = itemRewardNo;
	}

	public void setItemNo(String itemNo) {
		this.itemNo = itemNo;
	}

	public void setItemType(int itemType) {
		this.itemType = itemType;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}

	public void setDropRate(double dropRate) {
		this.dropRate = dropRate;
	}

}
