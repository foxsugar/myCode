package com.crystalcg.gamedev.util.cache.domain;

/**
 * 城池等级信息表
 * 
 * @author xuzhongxing
 * 
 */
public class StaticCity {
	private int level;// 城池等级
	private int experience;// 升级的需求经验
	private int bulidingNum;// 可建造地块的数目
	private int resourceNum;// 在该等级下城郊拥有的资源点数目
	private int tributaryNum;// 在该等级下拥有的分城数目
	private int age;// 时代

	public int getLevel() {
		return level;
	}

	public int getExperience() {
		return experience;
	}

	public int getBulidingNum() {
		return bulidingNum;
	}

	public int getResourceNum() {
		return resourceNum;
	}

	public int getTributaryNum() {
		return tributaryNum;
	}

	public int getAge() {
		return age;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public void setExperience(int experience) {
		this.experience = experience;
	}

	public void setBulidingNum(int bulidingNum) {
		this.bulidingNum = bulidingNum;
	}

	public void setResourceNum(int resourceNum) {
		this.resourceNum = resourceNum;
	}

	public void setTributaryNum(int tributaryNum) {
		this.tributaryNum = tributaryNum;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
