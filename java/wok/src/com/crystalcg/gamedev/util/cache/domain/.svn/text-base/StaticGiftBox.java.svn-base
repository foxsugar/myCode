package com.crystalcg.gamedev.util.cache.domain;

/**
 * 礼包静态表
 * @author jinganyang
 *
 */
public class StaticGiftBox {
	
	private int id;
	private String giftNo; // 礼包编号
	private int needLevel; // 需求等级
	private String includeItemNo; // 包含物品编号
	private int includeItemType; // 包含物品类型
	private int includeItemAmount; // 包含物品数量
	
	
	public int getId() {
		return id;
	}
	public String getGiftNo() {
		return giftNo;
	}
	public int getNeedLevel() {
		return needLevel;
	}
	public String getIncludeItemNo() {
		return includeItemNo;
	}
	public int getIncludeItemType() {
		return includeItemType;
	}
	public int getIncludeItemAmount() {
		return includeItemAmount;
	}
	public void setId(int id) {
		this.id = id;
	}
	public void setGiftNo(String giftNo) {
		this.giftNo = giftNo;
	}
	public void setNeedLevel(int needLevel) {
		this.needLevel = needLevel;
	}
	public void setIncludeItemNo(String includeItemNo) {
		this.includeItemNo = includeItemNo;
	}
	public void setIncludeItemType(int includeItemType) {
		this.includeItemType = includeItemType;
	}
	public void setIncludeItemAmount(int includeItemAmount) {
		this.includeItemAmount = includeItemAmount;
	}

}
