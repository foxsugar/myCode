package com.crystalcg.gamedev.util.cache.domain;

/**
 * 系统头像
 * @author lvxiaohui
 *
 */
public class StaticPic{
	private int id;
	private String name;//头像地址名称
	private int type;//是否是系统自选图像1是，0否
	private int gender;
	private int requirePType;//特权类型 0无1vip
	private int requirePValue;//特权数值（等级）
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getGender() {
		return gender;
	}
	public void setGender(int gender) {
		this.gender = gender;
	}
	public int getRequirePType() {
		return requirePType;
	}
	public void setRequirePType(int requirePType) {
		this.requirePType = requirePType;
	}
	public int getRequirePValue() {
		return requirePValue;
	}
	public void setRequirePValue(int requirePValue) {
		this.requirePValue = requirePValue;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
}
