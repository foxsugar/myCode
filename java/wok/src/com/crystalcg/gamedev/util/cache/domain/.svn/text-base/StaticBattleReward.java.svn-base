package com.crystalcg.gamedev.util.cache.domain;

/**
 * 战斗奖励静态信息
 * 
 * @author jinganyang
 * 
 */
public class StaticBattleReward {

	private String enemyNo;// 野怪编号
	private int exploitReward;// 功勋奖励
	private int expReward; // 经验奖励
	private int foodReward;// 粮食奖励
	private int woodReward; // 木材奖励
	private int stoneReward; // 石料奖励
	private int ironoreReward; // 铁矿奖励
	private String articleReward1; // 物品奖励（单挑）
	private String articleReward2; // 物品奖励（讨伐）

	public String getEnemyNo() {
		return enemyNo;
	}

	public int getExploitReward() {
		return exploitReward;
	}

	public int getExpReward() {
		return expReward;
	}

	public int getFoodReward() {
		return foodReward;
	}

	public int getWoodReward() {
		return woodReward;
	}

	public int getStoneReward() {
		return stoneReward;
	}

	public int getIronoreReward() {
		return ironoreReward;
	}

	public String getArticleReward1() {
		return articleReward1;
	}

	public String getArticleReward2() {
		return articleReward2;
	}

	public void setEnemyNo(String enemyNo) {
		this.enemyNo = enemyNo;
	}

	public void setExploitReward(int exploitReward) {
		this.exploitReward = exploitReward;
	}

	public void setExpReward(int expReward) {
		this.expReward = expReward;
	}

	public void setFoodReward(int foodReward) {
		this.foodReward = foodReward;
	}

	public void setWoodReward(int woodReward) {
		this.woodReward = woodReward;
	}

	public void setStoneReward(int stoneReward) {
		this.stoneReward = stoneReward;
	}

	public void setIronoreReward(int ironoreReward) {
		this.ironoreReward = ironoreReward;
	}

	public void setArticleReward1(String articleReward1) {
		this.articleReward1 = articleReward1;
	}

	public void setArticleReward2(String articleReward2) {
		this.articleReward2 = articleReward2;
	}

}
