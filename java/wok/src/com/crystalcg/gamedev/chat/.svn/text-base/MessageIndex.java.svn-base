package com.crystalcg.gamedev.chat;

public class MessageIndex {
	private int worldIndex;
	private int systemIndex;
	private int countryIndex;
	private int allianceIndex;
	private int personalIndex;
	public int getWorldIndex() {
		return worldIndex;
	}
	public int getCountryIndex() {
		return countryIndex;
	}
	public int getAllianceIndex() {
		return allianceIndex;
	}
	public int getPersonalIndex() {
		return personalIndex;
	}
	public void setWorldIndex(int worldIndex) {
		this.worldIndex = worldIndex;
	}
	public void setCountryIndex(int countryIndex) {
		this.countryIndex = countryIndex;
	}
	public void setAllianceIndex(int allianceIndex) {
		this.allianceIndex = allianceIndex;
	}
	public void setPersonalIndex(int personalIndex) {
		this.personalIndex = personalIndex;
	}
	public int getSystemIndex() {
		return systemIndex;
	}
	public void setSystemIndex(int systemIndex) {
		this.systemIndex = systemIndex;
	}
}
