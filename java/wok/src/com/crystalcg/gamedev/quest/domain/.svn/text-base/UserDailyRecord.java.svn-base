package com.crystalcg.gamedev.quest.domain;

import java.util.Date;


public class UserDailyRecord {
	private int id;
	private int reputationAmount;
	private int heroAmount;
	private Date updateDate;
	private int characterId;
	
	public UserDailyRecord(){
		
	}
	public UserDailyRecord(int characterId, Date date){
		this.characterId = characterId;
		updateDate = date;
	}
	
	public int getId() {
		return id;
	}
	public int getReputationAmount() {
		return reputationAmount;
	}
	public int getHeroAmount() {
		return heroAmount;
	}
	public Date getUpdateDate() {
		return updateDate;
	}
	public void setId(int id) {
		this.id = id;
	}
	public void setReputationAmount(int reputationAmount) {
		this.reputationAmount = reputationAmount;
	}
	public void setHeroAmount(int heroAmount) {
		this.heroAmount = heroAmount;
	}
	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}
	public int getCharacterId() {
		return characterId;
	}
	public void setCharacterId(int characterId) {
		this.characterId = characterId;
	}
	
}
