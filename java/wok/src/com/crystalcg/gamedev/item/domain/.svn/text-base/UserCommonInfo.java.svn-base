package com.crystalcg.gamedev.item.domain;

/**
 * 用户物品（除装备外）共有属性
 * @author xuzhongxing
 *
 */
public class UserCommonInfo {
	
	protected int id;// 用户任务物品ID
	protected int characterId;// 玩家角色Id
	protected String itemNo;// 道具标识Id
	protected int itemType;// 道具类型
	protected int itemSubtype;//道具子类型
	protected int itemAmount;// 道具数量
	protected int isBound;// 道具是否绑定
	protected int itemPosition;// 道具位置状态：0国库，1武将，2邮件
	
	public int getId() {
		return id;
	}
	public int getCharacterId() {
		return characterId;
	}
	public String getItemNo() {
		return itemNo;
	}
	public int getItemType() {
		return itemType;
	}
	public int getItemAmount() {
		return itemAmount;
	}
	public int getIsBound() {
		return isBound;
	}
	public int getItemPosition() {
		return itemPosition;
	}
	public void setId(int id) {
		this.id = id;
	}
	public void setCharacterId(int characterId) {
		this.characterId = characterId;
	}
	public void setItemNo(String itemNo) {
		this.itemNo = itemNo;
	}
	public void setItemType(int itemType) {
		this.itemType = itemType;
	}
	public void setItemAmount(int itemAmount) {
		this.itemAmount = itemAmount;
	}
	public void setIsBound(int isBound) {
		this.isBound = isBound;
	}
	public void setItemPosition(int itemPosition) {
		this.itemPosition = itemPosition;
	}
	public int getItemSubtype() {
		return itemSubtype;
	}
	public void setItemSubtype(int itemSubtype) {
		this.itemSubtype = itemSubtype;
	}


}
