package com.crystalcg.gamedev.buildingFunction.domain;


/**
 * @author zhaibiao
 *
 */
public class UserWallHero {
	private int characterId;
	private double wallCombat;
	private String formationNo;
	private String heroId;

	public int getCharacterId() {
		return characterId;
	}
	public void setCharacterId(int characterId) {
		this.characterId = characterId;
	}
	public String getHeroId() {
		return heroId;
	}
	public void setHeroId(String heroId) {
		this.heroId = heroId;
	}
	public double getWallCombat() {
		return wallCombat;
	}
	public void setWallCombat(double wallCombat) {
		this.wallCombat = wallCombat;
	}
	public String getFormationNo() {
		return formationNo;
	}
	public void setFormationNo(String formationNo) {
		this.formationNo = formationNo;
	}

}
