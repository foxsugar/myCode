package com.crystalcg.gamedev.buildingFunction.domain;

/**
 * 官职
 * @author xuzhongxing
 *
 */
public class MilitaryRank {
	
	private int id;           //主键
	private String name;      //官职名
	private int level;        //官职等级
	private int numberLimit;  //武将个数限制
	private int needItem;     //晋升所需道具ID
	private int commandNumber;//统率
	
	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @return the level
	 */
	public int getLevel() {
		return level;
	}
	/**
	 * @return the numberLimit
	 */
	public int getNumberLimit() {
		return numberLimit;
	}
	/**
	 * @return the needItem
	 */
	public int getNeedItem() {
		return needItem;
	}
	/**
	 * @return the commandNumber
	 */
	public int getCommandNumber() {
		return commandNumber;
	}
	/**
	 * @param id the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @param level the level to set
	 */
	public void setLevel(int level) {
		this.level = level;
	}
	/**
	 * @param numberLimit the numberLimit to set
	 */
	public void setNumberLimit(int numberLimit) {
		this.numberLimit = numberLimit;
	}
	/**
	 * @param needItem the needItem to set
	 */
	public void setNeedItem(int needItem) {
		this.needItem = needItem;
	}
	/**
	 * @param commandNumber the commandNumber to set
	 */
	public void setCommandNumber(int commandNumber) {
		this.commandNumber = commandNumber;
	}

}
